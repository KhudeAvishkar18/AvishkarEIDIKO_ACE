
CREATE COMPUTE MODULE MF_test_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		CALL CopyEntireMessage();
		RETURN TRUE;
	END;

	CREATE PROCEDURE CopyEntireMessage() BEGIN
		
		CREATE FIELD OutputRoot;
		CREATE LASTCHILD OF OutputRoot NAME 'HTTPInputHeader';
		CREATE LASTCHILD OF OutputRoot DOMAIN 'JSON';
		CREATE LASTCHILD OF OutputRoot NAME 'Properties';

		SET OutputRoot.JSON.Data = InputRoot.XMLNSC;
		SET OutputRoot.HTTPInputHeader = InputRoot.HTTPInputHeader;
		SET OutputRoot.Properties = InputRoot.Properties;
		
	END;
END MODULE;